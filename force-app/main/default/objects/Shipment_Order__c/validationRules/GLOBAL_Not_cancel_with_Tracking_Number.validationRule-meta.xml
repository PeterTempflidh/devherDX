<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>GLOBAL_Not_cancel_with_Tracking_Number</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT( $Permission.Exclude_from_Validation ),NOT(ISBLANK(Tracking_Number__c)),ISPICKVAL(Status__c, &apos;Canceled&apos;))</errorConditionFormula>
    <errorMessage>This order was already shipped and can&apos;t be canceled.</errorMessage>
</ValidationRule>
