<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Franchise_Type_Empty</fullName>
    <active>true</active>
    <errorConditionFormula>AND(Franchise_Connection__c != null, ISPICKVAL( Franchise_Type__c, &quot;&quot;),RecordType.DeveloperName = &apos;Yogiyo_Account&apos;)</errorConditionFormula>
