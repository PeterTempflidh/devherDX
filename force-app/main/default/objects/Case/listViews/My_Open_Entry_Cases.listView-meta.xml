<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>My_Open_Entry_Cases</fullName>
    <columns>CASES.CASE_NUMBER</columns>
    <columns>ACCOUNT.NAME</columns>
    <columns>CASES.SUBJECT</columns>
    <columns>CASES.TYPE</columns>
    <columns>CASES.STATUS</columns>
    <columns>CASES.PRIORITY</columns>
    <columns>CASES.CREATED_DATE</columns>
    <filterScope>Mine</filterScope>
    <filters>
        <field>CASES.RECORDTYPE</field>
        <operation>equals</operation>
        <value>Case.CD_Entry_Case,Case.DH_AU_Entry_Case,Case.Talabat_Entry_Case,Case.FI_Entry_Case,Case.SE_Entry_Case</value>
    </filters>
    <filters>
        <field>CASES.STATUS</field>
        <operation>notEqual</operation>
        <value>Closed</value>
    </filters>
    <label>My Open Entry Cases</label>
    <language>en_US</language>
    <sharedTo>
        <role>FI_Customer_Care_Team</role>
        <role>SE_Customer_Care_Team</role>
        <role>Talabat_Operation_Team</role>
    </sharedTo>
</ListView>
