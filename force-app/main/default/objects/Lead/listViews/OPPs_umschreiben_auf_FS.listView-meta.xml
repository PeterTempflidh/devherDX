<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>OPPs_umschreiben_auf_FS</fullName>
    <booleanFilter>(1 AND 2 AND 3 AND 4 AND 5) OR (1 AND 3 AND 4 AND 5 AND 6)</booleanFilter>
    <columns>FULL_NAME</columns>
    <columns>LEAD.COMPANY</columns>
    <columns>LEAD.STATE</columns>
    <columns>LEAD.EMAIL</columns>
    <columns>LEAD.STATUS</columns>
    <columns>LEAD.CREATED_DATE</columns>
    <columns>CORE.USERS.ALIAS</columns>
    <columns>LEAD.UNREAD</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>LEAD.RECORDTYPE</field>
        <operation>equals</operation>
        <value>Lead.DE_Lead</value>
    </filters>
    <filters>
        <field>LEAD.STATUS</field>
        <operation>equals</operation>
        <value>New,Contacted,Long-term follow-up</value>
    </filters>
    <filters>
        <field>LEAD.ZIP</field>
        <operation>startsWith</operation>
        <value>40,41,454,46,47</value>
    </filters>
    <filters>
        <field>LEAD.LEAD_SOURCE</field>
        <operation>notEqual</operation>
        <value>Inbound</value>
    </filters>
    <filters>
        <field>CORE.USERS.ALIAS</field>
        <operation>notEqual</operation>
        <value>C Kal,DE Sales Operations</value>
    </filters>
    <filters>
        <field>Migration_status__c</field>
        <operation>notEqual</operation>
        <value>Lost,Converted</value>
    </filters>
    <label>Leads umschreiben auf FS</label>
    <language>de</language>
    <sharedTo>
        <group>DE_Sales_Operations</group>
    </sharedTo>
</ListView>
